<map proto="1.5.0">
<name>Icy Peaks TDM</name>
<version>1.2.3</version>
<objective>Be the team with the most amount of kills.</objective>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="c2848337-b526-4db0-9130-fabd0a5c7cfb"/> <!-- jollee999 -->
    <author uuid="7cc1e52e-4385-4154-bc1f-57d95afa2cff"/> <!-- DanikasTheMan -->
</authors>
<contributors>
    <contributor uuid="c63aa30b-256a-47c2-9d1d-b3f78163b612" contribution="Building help"/> <!-- _DDD -->
    <contributor uuid="44888141-cfae-42cf-8a26-1c7bfb1c4d64" contribution="Building help"/> <!-- p1e2m3a4 -->
</contributors>
<teams>
    <team id="red" color="dark red" max="20">Red</team>
    <team id="blue" color="blue" max="20">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" material="stone sword"/>
        <item slot="1" material="bow"/>
        <item slot="2" material="golden apple"/>
        <item slot="3" amount="12" material="arrow"/>
        <item slot="8" amount="32" material="bread"/>
        <item slot="9" amount="32" damage="4" material="ink sack"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate enchantment="protection projectile:2" material="iron chestplate"/>
        <boots unbreakable="true" team-color="true" material="leather boots"/>
        <effect duration="5" amplifier="3">heal</effect>
        <effect duration="5" amplifier="3">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit" yaw="-135">
        <region>
            <point>-51.5,10,-6.5</point>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="135">
        <region>
            <point>158.5,10,-6.5</point>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <point>52.5,43,32.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>10m</time>
<score>
    <kills>1</kills>
</score>
<itemremove>
    <item>bread</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather boots</item>
    <item>arrow</item>
    <item>golden apple</item>
</itemremove>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<kill-rewards>
    <kill-reward>
        <item amount="6" material="arrow"/>
        <item material="exp bottle"/>
    </kill-reward>
</kill-rewards>
<timelock>off</timelock>
</map>
