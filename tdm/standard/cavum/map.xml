<map proto="1.5.0">
<name>Cavum</name>
<version>1.1.2</version>
<objective>Get the most kills in 10 minutes!</objective>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="621b2aa8-4e5f-4ed8-bd36-0ae2f8952a06"/> <!-- thestrangeboy1 -->
    <author uuid="15dc7f7a-40cd-4389-ad45-f8a33a09a394"/> <!-- moo88888888 -->
</authors>
<teams>
    <team id="blue" color="blue" max="14">Blue</team>
    <team id="red" color="dark red" max="14">Red</team>
    <team id="purple" color="dark purple" max="14">Purple</team>
    <team id="yellow" color="yellow" max="14">Yellow</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" unbreakable="true" material="iron sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="8" amount="16" material="arrow"/>
        <item slot="2" amount="32" material="golden carrot"/>
        <item slot="3" material="golden apple"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" enchantment="protection projectile:2" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" material="gold leggings"/>
        <boots unbreakable="true" enchantment="protection fall:2" material="iron boots"/>
        <effect duration="3">heal</effect>
        <effect duration="3" amplifier="5">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit" yaw="45">
        <region>
            <point>52.5,42,-45.5</point>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="-135">
        <region>
            <point>-52.5,42,45.5</point>
        </region>
    </spawn>
    <spawn team="yellow" kit="spawn-kit" yaw="-45">
        <region>
            <point>-45.5,42,-52.5</point>
        </region>
    </spawn>
    <spawn team="purple" kit="spawn-kit" yaw="135">
        <region>
            <point>45.5,42,52.5</point>
        </region>
    </spawn>
    <default>
        <region>
            <point>0,85,0</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>10m</time>
<score>
    <kills>1</kills>
</score>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>golden carrot</item>
    <item>leather chestplate</item>
    <item>iron chestplate</item>
    <item>chainmail boots</item>
    <item>iron boots</item>
    <item>leather helmet</item>
    <item>iron helmet</item>
    <item>gold leggings</item>
    <item>iron leggings</item>
    <item>watch</item>
    <item>item frame</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="iron ingot"/>
    </kill-reward>
</kill-rewards>
<timelock>on</timelock>
</map>
