<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
        <item amount="3" material="gold ingot"/>
        <kit>
            <effect duration="3">speed</effect>
        </kit>
    </kill-reward>
    <kill-reward>
        <filter>
            <kill-streak count="3" repeat="false"/>
        </filter>
        <item material="golden apple"/>
        <item amount="3" material="gold ingot"/>
    </kill-reward>
    <kill-reward>
        <filter>
            <kill-streak count="5" repeat="false"/>
        </filter>
        <item material="golden apple"/>
        <item amount="3" material="gold ingot"/>
    </kill-reward>
    <kill-reward>
        <filter>
            <kill-streak count="7" repeat="true"/>
        </filter>
        <item material="golden apple"/>
        <item amount="3" material="gold ingot"/>
    </kill-reward>
</kill-rewards>
<item-mods>
    <rule>
        <match>
            <material>gold helmet</material>
        </match>
        <modify unbreakable="true">
            <enchantment level="10">durability</enchantment>
        </modify>
    </rule>
    <rule>
        <match>
            <material>gold chestplate</material>
        </match>
        <modify unbreakable="true">
            <enchantment level="10">durability</enchantment>
        </modify>
    </rule>
    <rule>
        <match>
            <material>gold leggings</material>
        </match>
        <modify unbreakable="true">
            <enchantment level="10">durability</enchantment>
        </modify>
    </rule>
</item-mods>
