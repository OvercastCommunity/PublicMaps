<map proto="1.5.0">
<name>CotBot TDM</name>
<version>1.0.2</version>
<objective>Be the team with the most kills before 7 minutes is up</objective>
<authors>
    <author uuid="878de754-59dd-4828-9328-2bb1b848dc43" contribution="Basic map layout and XML author"/> <!-- MrM00se -->
    <author uuid="2a289d2a-d970-49c5-9a6c-01fc0264e317" contribution="'Gayed' up the map (Aesthetics)"/> <!-- Stealth5061 -->
    <author uuid="539d57c8-0a91-4800-9a09-b1fe789a8a87" contribution="General helper and on-hand bovine"/> <!-- MinimalCow -->
    <author uuid="d2dfbb91-9bf7-400b-9d45-29800862a916" contribution="Moral support and minor aesthetics"/> <!-- FlavorfulGecko5 -->
</authors>
<teams>
    <team id="cot" color="dark red" max="12">Cot</team>
    <team id="bot" color="blue" max="12">Bot</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" unbreakable="true" material="wood sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" amount="12" material="cooked beef"/>
        <item slot="3" damage="16453" material="potion"/>
        <item slot="4" damage="16453" material="potion"/>
        <item slot="5" damage="16453" material="potion"/>
        <item slot="8" amount="16" material="arrow"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" team-color="true" material="leather leggings"/>
        <boots unbreakable="true" enchantment="protection fall:4" team-color="true" material="leather boots"/>
    </kit>
</kits>
<spawns>
    <spawn team="cot" kit="spawn-kit" yaw="-90">
        <region>
            <cuboid min="-55,12,-4" max="-60,12,3"/>
        </region>
    </spawn>
    <spawn team="bot" kit="spawn-kit" yaw="90">
        <region>
            <cuboid min="55,12,2" max="62,12,-3"/>
        </region>
    </spawn>
    <default yaw="180">
        <region>
            <cuboid min="-2,22,38" max="3,22,36"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never">
        <region>
            <rectangle id="main-area" min="-75,21" max="75,-23"/>
        </region>
    </apply>
</regions>
<time>7m</time>
<score>    
    <kills>1</kills>
    <deaths>1</deaths>
</score>
<itemremove>
    <item>potion</item>
    <item>cooked beef</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<toolrepair>
    <tool>wood sword</tool>
    <tool>bow</tool>
</toolrepair>
<kill-rewards>
    <kill-reward>
        <item damage="16453" material="potion"/>
        <item amount="8" material="arrow"/>
    </kill-reward>
</kill-rewards>
</map>
