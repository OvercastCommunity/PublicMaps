<map proto="1.5.0">
<name>Sugar Rush</name>
<version>1.0.4</version>
<objective>Kill as many enemies you can in 10 minutes!</objective>
<authors>
    <author uuid="3143b9d9-cea7-465f-97fc-8de59fbbb3a4" contribution="Map author"/> <!-- DiamondAx -->
</authors>
<teams>
    <team id="red" color="dark red" max="35">Red</team>
    <team id="green" color="dark green" max="35">Green</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" material="stone sword"/>
        <item slot="1" enchantment="arrow infinite" material="bow"/>
        <item slot="2" amount="32" material="pumpkin pie"/>
        <item slot="28" material="arrow"/>
        <item slot="3" amount="2" name="Caramel Apple" material="golden apple"/>
        <helmet team-color="true" material="leather helmet"/>
        <chestplate team-color="true" material="leather chestplate"/>
        <leggings team-color="true" material="leather leggings"/>
        <boots enchantment="protection projectile" material="iron boots"/>
        <effect duration="5">heal</effect>
        <effect duration="5" amplifier="2">damage resistance</effect>
        <effect>speed</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region>
            <cuboid min="594,29,51" max="597,29,54"/>
        </region>
    </spawn>
    <spawn team="green" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="598,29,250" max="595,29,247"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <cuboid min="508,79,147" max="515,79,154"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>10m</time>
<score> 
    <kills>1</kills>
</score>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>pumpkin pie</item>
    <item>golden apple</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>iron boots</item>
    <item>glass bottle</item>
    <item>iron spade:15</item>
    <item>potion</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item damage="8229" material="potion"/> <!-- instant health 2 -->
    </kill-reward>
</kill-rewards>
</map>
