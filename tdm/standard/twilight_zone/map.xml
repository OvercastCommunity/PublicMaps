<map proto="1.5.0">
<name>Twilight Zone</name>
<version>1.5.4</version>
<objective>Kill the enemy players as much as you can before time runs out.</objective>
<authors>
    <author uuid="b8e37008-a61d-4452-9375-fb9872f5bfea" contribution="Map planner, terraforming, and ideas"/> <!-- daeshik -->
</authors>
<contributors>
    <contributor uuid="900b844e-747c-4d5b-b9e3-afa714ec9bd5" contribution="Ideas and buildings"/> <!-- UnPhair -->
    <contributor uuid="5bce6a2c-4010-4f5c-9e79-822fc164528d" contribution="Ideas, planning, and spawn kits"/> <!-- MK325 -->
    <contributor uuid="22ba442b-cf26-4d09-b290-8d4e50a551b2" contribution="XML"/> <!-- Dinner1111 -->
</contributors>
<teams>
    <team id="red" color="dark red" max="40">Red</team>
    <team id="blue" color="blue" max="40">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all" material="diamond sword"/>
        <item slot="1" enchantment="arrow damage" material="bow"/>
        <item slot="2" damage="16421" material="potion"/> <!-- Splash of Healing II -->
        <item slot="3" damage="16421" material="potion"/> <!-- Splash of Healing II -->
        <item slot="4" damage="16421" material="potion"/> <!-- Splash of Healing II -->
        <item slot="5" damage="8226" material="potion"/> <!-- Speed II -->
        <item slot="7" amount="2" material="ender pearl"/>
        <item slot="8" amount="32" material="cooked beef"/>
        <item slot="28" amount="16" material="arrow"/>
        <item slot="30" damage="8193" material="potion"/> <!-- Regen -->
        <item slot="31" damage="8193" material="potion"/> <!-- Regen -->
        <item slot="32" damage="8226" material="potion"/> <!-- Speed II -->
        <item slot="13" damage="16394" material="potion"/> <!-- Splash of slowness -->
        <item slot="22" damage="16394" material="potion"/> <!-- Splash of slowness -->
        <item slot="14" damage="16394" material="potion"/> <!-- Splash of slowness -->
        <item slot="23" damage="16394" material="potion"/> <!-- Splash of slowness -->
        <helmet material="diamond helmet"/>
        <chestplate material="diamond chestplate"/>
        <leggings material="diamond leggings"/>
        <boots enchantment="protection fall" material="diamond boots"/>
        <effect amplifier="4" duration="5">heal</effect>
        <effect duration="15">increase_damage</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region>
            <cuboid min="190,18,-97" max="183,18,-100"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="50,18,96" max="59,18,101"/>
        </region>
    </spawn>
    <default yaw="270">
        <region>
            <cuboid min="1,55,-0" max="1,55,2"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>10m</time>
<score>
    <kills>1</kills>
</score>
<toolrepair>
    <tool>diamond sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>diamond helmet</item>
    <item>diamond chestplate</item>
    <item>diamond leggings</item>
    <item>diamond boots</item>
    <item>cooked beef</item>
    <item>glass bottle</item>
    <item>ender pearl</item>
    <item>potion</item>
    <item>arrow</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item damage="16421" material="potion"/> <!-- Splash of Healing II -->
        <item damage="8226" material="potion"/> <!-- Speed II -->
        <item damage="8193" material="potion"/> <!-- Regen -->
        <item damage="16388" material="potion"/> <!-- Splash Poison :33 -->
        <item damage="16394" material="potion"/> <!-- Splash of slowness -->
        <item amount="8" material="arrow"/>
    </kill-reward>
</kill-rewards>
<timelock>on</timelock>
</map>
