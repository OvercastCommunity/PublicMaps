<map proto="1.5.0">
<name>Canyon: TDM</name>
<version>1.0.2</version>
<objective>Get the most kills after 7 minutes!</objective>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="be49b1a8-600e-4751-9725-a8aead777363"/> <!-- IzJust4Fun -->
    <author uuid="c660918b-4000-44d4-b735-d3fa6f166110"/> <!-- RED_DEAD -->
</authors>
<teams>
    <team id="red" color="dark red" max="20">Red</team>
    <team id="orange" color="gold" max="20">Orange</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="8" amount="12" material="arrow"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" material="chainmail leggings"/>
        <boots unbreakable="true" team-color="true" enchantment="protection fall:2" material="leather boots"/>
        <effect amplifier="10" duration="3s">damage resistance</effect>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region>
            <point>-1274.5,33,-1196.5</point>
        </region>
    </spawn>
    <spawn team="orange" kit="spawn-kit" yaw="180">
        <region>
            <point>-1282.5,33,-1100.5</point>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <point>-1278.5,38,-1148.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>golden apple</item>
    <item>arrow</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item amount="6" material="arrow"/>
    </kill-reward>
</kill-rewards>
<score>
    <kills>1</kills>
</score>
<time>7m</time>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
