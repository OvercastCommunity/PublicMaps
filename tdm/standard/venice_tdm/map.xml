<map proto="1.5.0">
<name>Venice</name>
<version>1.5</version>
<variant id="christmas" world="christmas" override="true">Festive Venice</variant>
<unless variant="christmas">
    <slug>venice_tdm</slug>
</unless>
<objective>Get the most kills in 10 minutes!</objective>
<time>10m</time>
<score>
    <kills>1</kills>
</score>
<include id="conquest"/>
<constant id="damage-resistance">5s</constant>
<constant id="respawn-timer">2s</constant>
<authors>
    <author uuid="900b844e-747c-4d5b-b9e3-afa714ec9bd5"/> <!-- UnPhair -->
    <author uuid="468c7d75-62e0-4d11-8fc1-4888965be11c"/> <!-- matt772 -->
</authors>
<contributors>
    <contributor uuid="9310b44c-55b2-4a51-8be6-151b379d3d4d" contribution="XML"/> <!-- legobmw99 -->
</contributors>
<teams>
    <unless variant="christmas">
        <team id="team-one" color="blue" max="40">Blue</team>
        <team id="team-two" color="dark red" max="40">Red</team>
    </unless>
    <if variant="christmas">
        <team id="team-one" color="green" max="40">Green</team>
        <team id="team-two" color="red" max="40">Red</team>
    </if>
</teams>
<kits>
    <kit id="spawn-kit" parent="base-kit">
        <clear/>
        <item slot="3" unbreakable="true" material="iron pickaxe">
            <enchantment>efficiency</enchantment>
        </item>
        <item slot="4" amount="8" material="wood"/>
        <item slot="5" amount="16" material="ladder"/>
        <item slot="6" material="boat"/>
        <action>
            <enchant-item ignore-metadata="true" enchantment="feather_falling" level="2">
                <find material="iron boots"/>
            </enchant-item>
        </action>
        <effect>speed</effect>
    </kit>
</kits>
<spawns>
    <default>
        <region yaw="315">
            <cylinder base="-104,58,-111" radius="5" height="0"/>
        </region>
    </default>
    <spawn team="team-one" kit="spawn-kit">
        <region>
            <cuboid min="7,12,-84" max="-6,12,-76"/>
        </region>
    </spawn>
    <spawn team="team-two" kit="spawn-kit">
        <region yaw="180">
            <cuboid min="-6,12,85" max="7,12,77"/>
        </region>
    </spawn>
</spawns>
<filters>
    <deny id="deny-melting-ice">
        <all>
            <cause>world</cause>
            <material>ice</material>
        </all>
    </deny>
</filters>
<regions>
    <apply block="deny-melting-ice"/>
</regions>
<itemkeep>
    <item>boat</item>
    <item>ladder</item>
    <item>wood</item>
</itemkeep>
<kill-rewards>
    <kill-reward>
        <item amount="4" material="wood"/>
    </kill-reward>
</kill-rewards>
<maxbuildheight>32</maxbuildheight>
</map>
