<kill-rewards>
    <kill-reward>
        <item amount="32" material="tnt"/>
        <item material="dispenser"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
    <damage ally="true" self="false" enemy="false" other="false">block explosion</damage>
</disabledamage>
</map>
