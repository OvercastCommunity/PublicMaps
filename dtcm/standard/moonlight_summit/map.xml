<map proto="1.4.2">
<name>Moonlight Summit</name>
<version>1.5.8</version>
<objective>Destroy 90% of all three of the enemy's monuments!</objective>
<authors>
    <author uuid="e37f18f1-117b-47db-8806-6d3a0854b518"/> <!-- Chicky -->
