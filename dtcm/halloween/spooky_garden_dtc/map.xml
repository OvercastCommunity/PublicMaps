        <item slot="5" amount="64" damage="10">stained clay</item>
        <chestplate color="7F3FB2" unbreakable="true">leather chestplate</chestplate>
        <boots color="7F3FB2" unbreakable="true">leather boots</boots>
    </kit>
</kits>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>leather boots</item>
    <item>step:7</item>
    <item>quartz block:2</item>
    <item>step:3</item>
    <item>cobblestone stairs</item>
    <item>seeds</item>
    <item>long grass:2</item>
    <item>brown mushroom</item>
