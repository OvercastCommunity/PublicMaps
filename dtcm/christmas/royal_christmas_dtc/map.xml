    <default yaw="-90">
        <cylinder base="-48.5,50,12.5" radius="1" height="0"/>
    </default>
    <spawn team="red" kit="red" yaw="180">
        <cylinder base="-48.5,11,173.5" radius="0" height="0"/>
    </spawn>
    <spawn team="blue" kit="blue" yaw="0">
        <cylinder base="-48.5,11,-150.5" radius="0" height="0"/>
    </spawn>
