    <item>dirt</item>
    <item>stone</item>
    <item>seeds</item>
    <item>long grass</item>
    <item>red rose</item>
    <item>stained clay</item>
    <item>chainmail chestplate</item>
    <item>leather leggings</item>
</itemremove>
