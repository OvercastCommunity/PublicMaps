        </shape>
        <ingredient symbol="G">iron ingot</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron sword" name="`2Jungle Sword" lore="`7[Server]: Forged by the author.|`b(obs) `9`kâˆž`r`bRuediger_LP`f: Sup!" unbreakable="true" show-unbreakable="false" show-attributes="false" show-other="false"/>
