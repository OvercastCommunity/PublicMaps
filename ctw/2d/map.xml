<?xml version="1.0"?>
<map proto="1.3.0">
<name>2D</name>
<version>1.0</version>
<objective>Capture the enemy wool and place it at your spawn!</objective>
<authors>
    <author uuid="dad8b95c-cf6a-44df-982e-8c8dd70201e0"/> <!-- ElectroidFilms -->
    <author uuid="e37f18f1-117b-47db-8806-6d3a0854b518"/> <!-- chickolympics -->
</authors>
<contributors>
    <contributor uuid="15dc7f7a-40cd-4389-ad45-f8a33a09a394" contribution="Aesthetics"/> <!-- moo88888888 -->
</contributors>
<rules>
    <rule>Spawn killing is allowed!</rule>
	<rule>Fall damage is disabled!</rule>
</rules>
<teams>
    <team max="8" color="dark red">Red</team>
    <team max="8" color="blue">Blue</team>
</teams>
<classes family="2D" sticky="false">
    <class name="Soldier" default="true" description="Heavy melee" icon="stone sword">
        <kit>
            <item slot="0">stone sword</item>
            <item slot="1" enchantment="arrow infinite;arrow damage">bow</item>
            <item slot="2" amount="2">golden apple</item>
            <item slot="28">arrow</item>
            <leggings>iron leggings</leggings>
            <helmet>leather helmet</helmet>
            <chestplate attributes="generic.maxHealth:add:4" enchantment="protection projectile">leather chestplate</chestplate>
            <boots>leather boots</boots>
            <potion duration="oo" amplifier="2" ambient="true">jump</potion>
            <potion duration="4">damage resistance</potion>
            <potion duration="2" amplifier="5">heal</potion>
            <double-jump/>
        </kit>
    </class>
    <class name="Alchemist" description="Witchcraft" icon="potion">
        <kit>
            <item slot="0">wood sword</item>
            <item slot="1" enchantment="arrow infinite:1">bow</item>
            <item slot="2" amount="2">golden apple</item>
            <item slot="3" amount="2" damage="16385" potions="regeneration:300:0:false">potion</item>
            <item slot="4" amount="3" damage="16388" potions="poison:100:0:false">potion</item>
            <item slot="5" amount="2" damage="16424" potions="blindness:100:3:false;confusion:100:5:false">potion</item>
            <item slot="28">arrow</item>
            <leggings>chainmail leggings</leggings>
            <helmet>leather helmet</helmet>
            <chestplate>gold chestplate</chestplate>
            <boots>leather boots</boots>
            <potion duration="oo" amplifier="2" ambient="true">jump</potion>
            <potion duration="4">damage resistance</potion>
            <potion duration="2" amplifier="5">heal</potion>
            <double-jump/>
        </kit>
    </class>
    <class name="Runner" description="Speed and Agility" icon="feather">
        <kit>
            <item slot="0">stone axe</item>
            <item slot="1" enchantment="arrow infinite:1">bow</item>
            <item slot="2" amount="2">golden apple</item>
            <item slot="3" attributes="generic.movementSpeed:add:0.03">feather</item>
            <item slot="28">arrow</item>
            <chestplate>chainmail chestplate</chestplate>
            <potion duration="oo" amplifier="2" ambient="true">jump</potion>
            <potion duration="4">damage resistance</potion>
            <potion duration="2" amplifier="5">heal</potion>
            <double-jump/>
        </kit>
    </class>
    <class name="Bomber" description="Explosive" icon="tnt">
        <kit>
            <item slot="0">wood sword</item>
            <item slot="1" enchantment="arrow infinite:1">bow</item>
            <item slot="2" amount="2">golden apple</item>
            <item slot="3" amount="6" grenade="true">snow ball</item>
            <item slot="5" amount="1" damage="50">monster egg</item>
            <item slot="28">arrow</item>
            <helmet>leather helmet</helmet>
            <chestplate enchantment="protection explosions:2">chainmail chestplate</chestplate>
            <leggings>iron leggings</leggings>
            <potion duration="oo" amplifier="2" ambient="true">jump</potion>
            <potion duration="4">damage resistance</potion>
            <potion duration="2" amplifier="5">heal</potion>
            <double-jump/>
        </kit>
    </class>
    <class name="Archer" description="Percision" icon="bow">
        <kit>
            <item slot="0" attributes="generic.attackDamage:add:2">wood sword</item>
            <item slot="1" enchantment="arrow infinite;arrow damage:2" attributes="generic.movementSpeed:add:-0.05">bow</item>
            <item slot="2" amount="2">golden apple</item>
            <item slot="28">arrow</item>
            <helmet>leather helmet</helmet>
            <chestplate enchantment="protection projectile:2">chainmail chestplate</chestplate>
            <leggings>leather leggings</leggings>
            <boots>chainmail boots</boots>
            <potion duration="oo" amplifier="2" ambient="true">jump</potion>
            <potion duration="4">damage resistance</potion>
            <potion duration="2" amplifier="5">heal</potion>
            <double-jump/>
        </kit>
    </class>
    <class name="Spider" description="Sticky and Silky" icon="web">
        <kit>
            <item slot="0">wood sword</item>
            <item slot="1" enchantment="arrow infinite">bow</item>
            <item slot="2" amount="2">golden apple</item>
            <item slot="3" amount="3">web</item>
            <item slot="28">arrow</item>
            <chestplate>chainmail chestplate</chestplate>
            <leggings>iron leggings</leggings>
            <potion duration="oo" amplifier="2" ambient="true">jump</potion>
            <potion duration="4">damage resistance</potion>
            <potion duration="2" amplifier="5">heal</potion>
            <double-jump/>
        </kit>
    </class>
</classes>
<spawns>
    <spawn team="red" yaw="-90">
        <point>-196.5,9,-1.5</point>
    </spawn>
    <spawn team="blue" yaw="90">
        <point>-3.5,9,-1.5</point>
    </spawn>
    <default yaw="-30">
        <point>-100.5,5,-1.5</point>
    </default>
</spawns>
<filters>
	<filter name="only-red" parents="deny-all">
		<allow>
            <team>red</team>
        </allow>
	</filter>
	<filter name="only-blue" parents="deny-all">
		<allow>
            <team>blue</team>
        </allow>
	</filter>
    <filter name="hack" parents="deny-all">
        <allow>
            <block>web</block>
        </allow>
    </filter>
</filters>
<lanes>
    <lane team="blue">
        <region name="map"/>
    </lane>
    <lane team="red">
        <region name="map"/>
    </lane>
</lanes>
<regions>
    <complement name="all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
        <block>-199,11,-2</block>
        <block>-2,11,-2</block>
    </complement>
    <cuboid name="map" min="-201,-oo,2" max="1,42,-3"/>
    <cuboid name="blue-wool-room" min="-9,23,-4" max="-3,30,1"/> <!-- Blue defends -->
    <cuboid name="red-wool-room" min="-191,23,-4" max="-197,30,1"/> <!-- Red defends -->
    <union name="blue-webs"> <!-- Allows for Red -->
        <block>-8,25,-2</block>
        <block>-8,26,-2</block>
    </union>
    <union name="red-webs"> <!-- Allows for Blue -->
        <block>-193,25,-2</block>
        <block>-193,26,-2</block>
    </union>
    <!-- Applied -->
    <apply block="hack deny-world">
        <region name="all"/>
    </apply>
    <apply enter="only-blue" message="This is a `9BLUE TEAM `conly wool room!">
        <region name="red-wool-room"/>
    </apply>
    <apply enter="only-red" message="This is a `4RED TEAM `conly wool room!">
        <region name="blue-wool-room"/>
    </apply>
    <apply block-break="only-blue" block-place="deny-all">
        <region name="red-webs"/>
    </apply>
    <apply block-break="only-red" block-place="deny-all">
        <region name="blue-webs"/>
    </apply>
</regions>
<wools>
   <wool team="blue" color="red">                  
        <block>-2,11,-2</block>
   </wool>
   <wool team="red" color="blue">
        <block>-199,11,-2</block>
   </wool>
</wools>
<itemremove>
    <item>stone sword</item>
    <item>wood sword</item>
    <item>stone axe</item>
    <item>chainmail chestplate</item>
    <item>gold chestplate</item>
    <item>chainmail leggings</item>
    <item>bow</item>
    <item>golden apple</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>iron leggings</item>
    <item>leather boots</item>
    <item>string</item>
    <item>potion</item>
    <item>monster egg</item>
    <item>snow ball</item>
    <item>sulphur</item>
    <item>leather leggings</item>
    <item>chainmail boots</item>
    <item>feather</item>
    <item>tnt</item>
    <item>iron door</item>
    <item>iron door block</item>
    <item>web</item>
</itemremove>
<killreward>
    <item amount="1">golden apple</item>
</killreward>
<timelock>on</timelock>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
<mobs>
    <filter parents="deny-all">
        <allow>
            <mob>creeper</mob>
            <spawn>spawner egg</spawn>
        </allow>
    </filter>
</mobs>
</map>
