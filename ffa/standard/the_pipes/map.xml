<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>shears</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>leather chestplate</item>
    <item>chainmail chestplate</item>
    <item>leather chestplate</item>
    <item>golden apple</item>
    <item>leaves</item>
    <item>sapling</item>
    <item>apple</item>
    <item>long grass</item>
    <item>red rose</item>
    <item>carpet</item>
    <item>wool</item>
    <item>torch</item>
    <item>wood</item>
    <item>arrow</item>
    <item>stick</item>
</itemremove>
<block-drops>
    <rule filter="allowed-blocks">
        <drops>
            <item chance="0" material="red rose"/>
        </drops>
    </rule>
</block-drops>
<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
    </kill-reward>
</kill-rewards>
<disabledamage>
    <damage>fall</damage>
