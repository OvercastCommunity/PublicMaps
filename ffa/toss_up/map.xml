<map proto="1.5.0">
<name>Toss Up</name>
<version>1.4.6</version>
<variant id="oitq" slug="toss_up_oitq">One in the Quiver</variant>
<objective>Kill as many players as you can before ${time-limit} minutes are up!</objective>
<created>2016-03-27</created>
<authors>
    <author uuid="fe3608b7-d105-4029-8800-34b3147065b6"/> <!-- rockymine -->
    <author uuid="105f6a09-f533-412b-93cb-501601763c11"/> <!-- JTr -->
</authors>
<players max="5" colors="true"/>
<constant id="time-limit">5</constant>
<filters>
    <any id="allowed-blocks">
        <material>leaves</material>
        <material>leaves 2</material>
        <material>torch</material>
        <material>long grass</material>
    </any>
</filters>
<regions>
    <apply velocity="0.0,2,0.0">
        <region>
            <block>3,10,-106</block>
            <block>-24,10,-86</block>
            <block>2,10,-65</block>
            <block>29,9,-95</block>
        </region>
    </apply>
    <apply block-place="never" block-break="allowed-blocks"/>
</regions>
<if variant="default">
    <gamemode>ffa</gamemode>
    <time result="score">${time-limit}m</time>
    <score>
        <kills>1</kills>
    </score>
    <constant id="base-kit">spawn-kit</constant>
    <include id="gapple-kill-reward"/>
    <kits>
        <kit id="spawn-kit" parents="default-kit">
            <clear/>
            <item slot="0" unbreakable="true" material="stone sword"/>
            <item slot="1" unbreakable="true" material="bow"/>
            <item slot="2" material="golden apple"/>
            <item slot="8" amount="16">arrow</item>
            <helmet locked="true" unbreakable="true" team-color="true" material="leather helmet"/>
            <chestplate locked="true" unbreakable="true" team-color="true" material="leather chestplate"/>
            <leggings locked="true" unbreakable="true" material="chainmail leggings"/>
            <boots locked="true" unbreakable="true" team-color="true" material="leather boots"/>
            <effect duration="2s" amplifier="10">damage resistance</effect>
        </kit>
    </kits>
    <toolrepair>
        <tool>stone sword</tool>
        <tool>bow</tool>
    </toolrepair>
    <itemremove>
        <item>leather helmet</item>
        <item>leather chestplate</item>
        <item>chainmail leggings</item>
        <item>leather boots</item>
        <item>arrow</item>
        <item>golden apple</item>
    </itemremove>
    <kill-rewards>
        <kill-reward>
            <item amount="8" material="arrow"/>
        </kill-reward>
    </kill-rewards>
    <respawn auto="true"/>
    <hunger>
        <depletion>off</depletion>
    </hunger>
    <disabledamage>
        <damage>fall</damage>
    </disabledamage>
</if>
<if variant="oitq">
    <include id="oitq"/>
    <constant id="extra-kit">gamemode-kit</constant>
    <kits>
        <kit id="gamemode-kit" parent="default-kit">
            <game-mode>survival</game-mode>
        </kit>
    </kits>
</if>
<spawns>
    <default>
        <kit id="default-kit">
            <effect>night vision</effect>
        </kit>
        <region yaw="-180">
            <cuboid min="0,18,-82" max="-2,21,-80"/>
        </region>
    </default>
    <spawn kit="${base-kit}" safe="true" spread="true">
        <regions>
            <point yaw="-85">-22.5,11,-86.5</point>
            <point yaw="93">-21.5,11,-84.5</point>
            <point yaw="45">0.5,11,-111.5</point>
            <point yaw="-90">-2.5,11,-106.5</point>
            <point yaw="62">-6.5,12,-112.5</point>
            <point yaw="-38">-21.5,11,-111.5</point>
            <point yaw="3.3">-28.5,22,-98.5</point>
            <point yaw="-179">-25.5,18,-58.5</point>
            <point yaw="50">30.5,21,-75.5</point>
            <point yaw="-89">-21.5,22,-86.5</point>
            <point yaw="-133">7.5,18,-53.5</point>
            <point yaw="-137">24.5,11,-55.5</point>
            <point yaw="178">3.5,11,-65.5</point>
            <point yaw="-164">-13.5,11,-52.5</point>
            <point yaw="-96">-29.5,11,-69.5</point>
        </regions>
    </spawn>
</spawns>
<itemremove>
    <item>stick</item>
    <item>seeds</item>
    <item>dirt</item>
    <item>armor stand</item>
    <item>sapling</item>
    <item>torch</item>
</itemremove>
<disabledamage>
    <damage>suffocation</damage>
</disabledamage>
</map>
