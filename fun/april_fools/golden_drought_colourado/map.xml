    <apply enter="only-lime" region="lime-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-cyan" region="cyan-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-orange" region="orange-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-magenta" region="magenta-spawn" message="You may not enter the enemy's spawn!"/>
    <apply block="wool-sping-filter" region="wool-regen" message="You may not edit the wool spring!"/>
    <apply block="never" region="spawns" message="You may not edit spawn!"/>
    <apply block="not-void" region="not-build-area" message="You may not edit the void!"/>
    <apply kit="reset-resistance-kit" region="not-spawns"/>
    <apply block-physics="deny-redstone" region="spawns"/>
</regions>
<renewables>
