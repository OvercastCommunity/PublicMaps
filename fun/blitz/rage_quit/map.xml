<map proto="1.5.0">
<name>Rage Quit</name>
<version>1.0.7</version>
<objective>Eliminate the other team before 5 minutes is up</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="ef4ea031-998f-4ec9-b7b6-1bdd428bcef8"/> <!-- Plastix -->
    <author uuid="3ce426cd-2ded-4f83-a9bb-46a57d68d9a5"/> <!-- McYukon -->
</authors>
<contributors>
    <contributor contribution="Beta testing and initial feedback">(Team) Starfish</contributor>
</contributors>
<teams>
    <team id="red" color="dark red" max="20">Red</team>
    <team id="blue" color="blue" max="20">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" lore="`91-Hit Kill!" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" lore="`91-Shot Kill!" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet locked="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" team-color="true" material="leather leggings"/>
        <boots locked="true" team-color="true" material="leather boots"/>
        <effect duration="5">heal</effect>
        <effect duration="5" amplifier="4">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit" yaw="45">
        <region>
            <cuboid min="43,9,-42" max="47,9,-44"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="-135">
        <region>
            <cuboid min="-42,9,43" max="-44,9,47"/>
        </region>
    </spawn>
    <default yaw="180">
        <region>
            <cylinder base="-4,49,-73" radius="2" height="0"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
