<map proto="1.5.0">
<name>The 6th Law</name>
<version>1.0.6</version>
<created>2013-07-04</created>
<objective>Eliminate the other team before 5 minutes is up</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="2ca8072f-74be-4798-85b8-bbce03aa91af"/> <!-- Tywnis -->
</authors>
<teams>
    <team id="red" color="dark red" max="20">Red</team>
    <team id="blue" color="blue" max="20">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" lore="`61-Hit Kill!" material="gold sword"/>
        <item slot="1" enchantment="arrow damage:10;arrow fire:10" lore="`61-Shot Kill!" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet locked="true" material="gold helmet"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" team-color="true" material="leather leggings"/>
        <boots locked="true" material="gold boots"/>
        <effect duration="5">heal</effect>
        <effect duration="5" amplifier="4">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit" yaw="90">
        <region>
            <cylinder base="14.5,25,44.5" radius="3" height="0"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="90">
        <region>
            <cylinder base="14.5,25,-43.5" radius="3" height="0"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <cylinder base="67.5,12,0.5" radius="1" height="0"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<toolrepair>
    <tool>gold sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>gold helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>gold boots</item>
    <item>glass bottle</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
