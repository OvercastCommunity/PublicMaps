<map proto="1.5.0">
<name>Java: Rage</name>
<version>1.0.4</version>
<objective>Eliminate the enemy team!</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="2e05eb91-e64b-46f8-8a49-368d205d26fd"/> <!-- Arkylic -->
    <author uuid="6bc5c322-91b5-4bab-acf2-d36ca53f1efc"/> <!-- FluffinAllNight -->
</authors>
<teams>
   <team id="red" color="red" max="20" max-overfill="25">Red</team>
   <team id="yellow" color="yellow" max="20" max-overfill="25">Yellow</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <item slot="3" amount="16" material="bread"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <boots unbreakable="true" team-color="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region yaw="45">
            <point>89.5,37,18.5</point>
        </region>
    </spawn>
    <spawn team="yellow" kit="spawn-kit">
        <region yaw="-90">
            <point>24.5,37,88.5</point>
        </region>
    </spawn>
    <default>
        <region yaw="90">
            <cuboid min="-18,94,52" max="-15,94,55"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>4m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<itemremove>
    <item>bread</item>
    <item>iron sword</item>
    <item>arrow</item>
    <item>bow</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
