<map proto="1.5.0">
<name>Aerugo</name>
<version>1.0.3</version>
<created>2014-06-14</created>
<objective>Eliminate the other team before 5 minutes!</objective>
<gamemode>blitz</gamemode>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="e3fdaea1-8a19-4378-8ad7-41cf63f5d141"/> <!-- Avaleer -->
</authors>
<contributors>
    <contributor uuid="ba4a93aa-4935-4a71-bab2-a58f9a124e7b" contribution="XML"/> <!-- ItsMiiOlly -->
</contributors>
<teams>
    <team id="green" color="green" max="12">Green</team>
    <team id="blue" color="blue" max="12">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" material="stone sword"/>
        <item slot="1" material="bow"/>
        <item slot="28" amount="32" material="arrow"/>
        <item slot="2" material="golden apple"/>
        <item slot="3" amount="16" material="golden carrot"/>
        <helmet team-color="true" material="leather helmet"/>
        <chestplate team-color="true" enchantment="protection projectile:2" material="leather chestplate"/>
        <leggings material="gold leggings"/>
        <boots team-color="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="green" kit="spawn-kit" yaw="-180">
        <region>
            <cylinder base="-337.5,5,-614.5" radius="2" height="0"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit">
        <region>
            <cylinder base="-338.5,5,-657.5" radius="2" height="0"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <cuboid min="-315,22,-636" max="-314,22,-635"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<itemremove>
    <item>arrow</item>
    <item>golden apple</item>
    <item>golden carrot</item>
    <item>gold leggings</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
</itemremove>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<timelock>on</timelock>
</map>
