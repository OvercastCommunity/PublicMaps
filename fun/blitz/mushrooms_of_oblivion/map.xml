<map proto="1.5.0">
<name>Mushrooms of Oblivion</name>
<version>1.5.5</version>
<objective>Eliminate the opposing team in under 5 minutes.</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="c65e66b3-e932-4b33-b907-fe4916806076"/> <!-- Draco_Rogue -->
</authors>
<teams>
   <team id="green" color="green" max="25">Green</team>
   <team id="purple" color="dark purple" max="25">Purple</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:127" name="`3`l1-Hit kill!" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:127" name="`3`l1-Shot Kill!" material="bow"/>
        <item slot="2" material="arrow"/>
        <chestplate locked="true" name="`dArmor" team-color="true" material="leather chestplate"/>
        <effect duration="5">heal</effect>
        <effect duration="5" amplifier="4">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="purple" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="0,113,36" max="-3,113,41"/>
        </region>
    </spawn>
    <spawn team="green" kit="spawn-kit">
        <region>
            <cuboid min="0,113,-36" max="-3,113,-41"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <cuboid min="71,162,-5" max="80,162,4"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<itemremove>
    <item>leather chestplate</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
