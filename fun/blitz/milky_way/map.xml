<map proto="1.5.0">
<name>Milky Way</name>
<version>1.0.4</version>
<created>2015-06-14</created>
<objective>Kill the other team's players!</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="eff8ed8e-4eae-4fe3-9bed-0485bc45d64e"/> <!-- Poseyidon -->
</authors>
<teams>
   <team id="green-team" color="green" max="5" max-overfill="6">Green</team>
   <team id="blue-team" color="blue" max="5" max-overfill="6">Blue</team>
   <team id="orange-team" color="gold" max="5" max-overfill="6">Orange</team>
   <team id="pink-team" color="light purple" max="5" max-overfill="6">Pink</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <chestplate team-color="true" material="leather chestplate"/>
        <leggings material="gold leggings"/>
        <boots team-color="true" material="leather boots"/>
        <effect duration="5">heal</effect>
        <effect duration="5" amplifier="4">damage resistance</effect>
        <effect amplifier="2" ambient="true">speed</effect>
        <effect amplifier="2" ambient="true">jump</effect>
        <effect duration="3" amplifier="100">damage resistance</effect>
        <effect duration="3" amplifier="100">heal</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="green-team" kit="spawn-kit">
        <region yaw="-45">
            <point>808.5,19,-645.5</point>
        </region>
    </spawn>
    <spawn team="blue-team" kit="spawn-kit">
        <region yaw="135">
            <point>886.5,19,-567.5</point>
        </region>
    </spawn>
    <spawn team="orange-team" kit="spawn-kit">
        <region yaw="-135">
            <point>808.5,19,-567.5</point>
        </region>
    </spawn>
    <spawn team="pink-team" kit="spawn-kit">
        <region yaw="45">
            <point>886.5,19,-645.5</point>
        </region>
    </spawn>
    <default>
        <region>
            <point>847.5,57,-606.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>2</lives>
</blitz>
<rage/>
<kill-rewards>
    <kill-reward>
    	<filter>
            <kill-streak count="1" repeat="true"/>
        </filter>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>gold leggings</item>
    <item>leather boots</item>
    <item>iron sword</item>
    <item>bow</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<respawn auto="true"/>
</map>
