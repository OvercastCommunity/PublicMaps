<map proto="1.5.0">
<name>Wonderland</name> 
<version>1.0.3</version> 
<objective>Eliminate the enemy team before time runs out!</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="e79fbdc9-627e-4dac-97a6-81a3046220d7"/> <!-- AtlasGames -->
</authors>
<teams>
   <team id="red" color="red" max="25" max-overfill="30">Red</team>
   <team id="blue" color="blue" max="25" max-overfill="30">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <item slot="3" amount="4" material="bread"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <boots unbreakable="true" team-color="true" material="leather boots"/>
        <effect duration="8" amplifier="25">heal</effect>
        <effect duration="8" amplifier="25">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region yaw="-90">
            <point>101.5,82,0.5</point>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit">
        <region yaw="90">
            <point>278.5,82,-0.5</point>
        </region>
    </spawn>
    <default>
        <region>
            <point>190,82,-43</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>6m</time>
<blitz>
    <lives>2</lives>
</blitz>
<rage/>
<itemremove>
    <item>bread</item>
    <item>iron sword</item>
    <item>arrow</item>
    <item>bow</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
