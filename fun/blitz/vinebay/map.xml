<map proto="1.5.0">
<name>Vinebay</name>
<version>1.0.3</version>
<objective>Eliminate the other team before 5 minutes is up</objective>
<created>2015-04-13</created>
<gamemode>br</gamemode>
<authors>
    <author uuid="c2848337-b526-4db0-9130-fabd0a5c7cfb"/> <!-- Rasta_Jol -->
    <author uuid="fe3608b7-d105-4029-8800-34b3147065b6"/> <!-- rockymine -->
</authors>
<teams>
    <team id="blue-team" color="blue" max="14" max-overfill="16">Blue</team>
    <team id="red-team" color="red" max="14" max-overfill="16">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <item slot="8" amount="32" material="cooked fish"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <boots unbreakable="true" team-color="true" material="leather boots"/>
        <effect duration="5" amplifier="25">heal</effect>
        <effect duration="5" amplifier="25">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="spawn-kit">
        <region>
            <cuboid min="-224.5,10,1.5" max="-226.5,10,-0.5" />
        </region>
    </spawn>
    <spawn team="red-team" kit="spawn-kit">
        <region yaw="180">
            <cuboid min="-224.5,10,127.5" max="-226.5,10,125.5" />
        </region>
    </spawn>
    <default>
        <region yaw="90">
            <cuboid min="-224.5,33,62.5" max="-226.5,33,64.5" />
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>leather boots</item>
    <item>cooked fish</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<timelock>on</timelock>
</map>
