<map proto="1.4.2">
<name>Bouncy Stadium</name>
<version>1.0.2</version>
<objective>Eliminate the other team before 5 minutes is up</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="95763a18-eb88-44e7-8a10-780acffceae8"/> <!-- Creephunter -->
</authors>
<contributors>
    <contributor uuid="c26e7a7f-b4ce-4ebc-ac4d-f60691915d23" contribution="XML and some gameplay help"/> <!-- Kroest -->
</contributors>
<teams>
    <team id="green" color="green" max="20" max-overfill="30">Green</team>
    <team id="red" color="red" max="20" max-overfill="30">Red</team>
</teams>
<kits>
    <kit id="obs">
        <effect>night vision</effect>
    </kit>
    <kit id="spawn-kit" parents="obs">
        <item slot="0" enchantment="damage all:100" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:100" material="bow"/>
        <item slot="2" material="arrow"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" team-color="true" material="leather leggings"/>
        <boots locked="true" team-color="true" material="leather boots"/>
        <effect duration="3" amplifier="100">heal</effect>
        <effect duration="3" amplifier="100">damage resistance</effect>
        <effect duration="oo" amplifier="2">speed</effect>
        <effect duration="oo" amplifier="2">jump</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="green" kit="spawn-kit" yaw="90">
        <region>
            <point>65.5,30,-55.5</point>
        </region>
    </spawn>
    <spawn team="red" kit="spawn-kit" yaw="-90">
        <region>
            <point>-64.5,30,56.5</point>
        </region>
    </spawn>
    <default yaw="135" kit="obs">
        <region>
            <point>53.5,44,51.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
    <apply velocity="@0,2.2,0">
        <region>
            <cylinder base="-57,10,40" radius="2.2" height="2"/>
        </region>
    </apply>
    <apply velocity="@0,2.2,0">
        <region>
            <cylinder base="58,10,-39" radius="2.2" height="2"/>
        </region>
    </apply>
    <apply velocity="@-1.5,1.4,1.5">
        <region>
            <cylinder base="37.5,12,-36.5" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="@1.5,1.4,-1.5">
        <region>
            <cylinder base="-36.5,12,37.5" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="@-0.5,1.4,-5">
        <region>
            <cylinder base="-50.5,10,17.5" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="@5,1.4,0.5">
        <region>
            <cylinder base="-16.5,10,51.5" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="@0.5,1.4,5">
        <region>
            <cylinder base="51.5,10,-16.5" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="@-5,1.4,-0.5">
        <region>
            <cylinder base="17.5,10,-50.5" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="@-3,2,-3">
        <region>
            <cylinder base="37,14,37" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="@3,2,3">
        <region>
            <cylinder base="-36,14,-36" radius="2" height="2"/>
        </region>
    </apply>
    <apply velocity="0,1.3,0">
        <region>
            <cuboid min="-32,9,-32" max="33,10.5,33"/>
        </region>
    </apply>
</regions>
<time>5m</time>
<blitz>
    <lives>2</lives>
</blitz>
<rage/>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>iron sword</item>
    <item>bow</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
