<map proto="1.5.0">
<name>Grimsnes</name>
<version>1.0.7</version>
<objective>Eliminate the enemy team within 6 minutes!</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="a02e9415-05bc-40f8-be9f-7b1b521b6a0e"/> <!-- CacklingPanda -->
</authors>
<contributors>
    <contributor uuid="e39fe3bc-b80e-46d2-81fe-d18042471989" contribution="Random feedback and minor help with snow"/> <!-- EnarRikardz -->
</contributors>
<teams>
    <team id="blue" color="blue" max="25">Blue</team>
    <team id="red" color="red" max="25">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" name="`l`bA Pointy Object" material="diamond sword"/>
        <item slot="1" enchantment="arrow damage:10" name="`l`bOne Shot, One Kill" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet locked="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" team-color="true" material="leather leggings"/>
        <boots locked="true" team-color="true" material="leather boots"/>
        <effect duration="5">heal</effect>
        <effect duration="5">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="blue" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="682,12,162" max="689,12,169"/>
        </region>
    </spawn>
    <spawn team="red" kit="spawn-kit">
        <region>
            <cuboid min="682,12,64" max="689,12,71"/>
        </region>
    </spawn>
    <default yaw="90">
        <region>
            <cuboid min="601,51,120" max="610,51,113"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<toolrepair>
    <tool>diamond sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<respawn auto="true"/>
</map>
