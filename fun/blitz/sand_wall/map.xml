<map proto="1.5.0">
<name>Sand Wall</name>
<version>1.0.4</version>
<objective>Eliminate the enemy team!</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="5198dc37-537f-4ea5-8ee8-1e0952d4a36b"/> <!-- _3Each -->
</authors>
<contributors>
    <contributor uuid="a23a1d91-7e97-40b4-8f5e-1ab82d8f5ade" contribution="XML"/> <!-- chle_ -->
</contributors>
<rules>
    <rule>Do not prolong the match by hiding!</rule>
</rules>
<teams>
    <team id="red" color="dark red" max="12" max-overfill="15">Red</team>
    <team id="blue" color="blue" max="12" max-overfill="15">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" name="`bOne hit kill!" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" name="`bOne shot kill!" material="bow"/>
        <item slot="8" material="arrow"/>
        <helmet unbreakable="true" locked="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" locked="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" locked="true" team-color="true" material="leather leggings"/>
        <boots unbreakable="true" locked="true" team-color="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region yaw="180">
            <cylinder base="-1162.5,18,1188.5" radius="1" height="1"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit">
        <region>
            <cylinder base="-1162.5,18,1088.5" radius="1" height="1"/>
        </region>
    </spawn>
    <default>
        <region yaw="90">
            <cylinder base="-1215.5,47,1134.5" radius="1" height="1"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn auto="true"/>
<timelock>on</timelock>
</map>
