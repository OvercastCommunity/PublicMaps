<map proto="1.5.0">
<name>Secluded Oasis</name>
<version>1.2.6</version>
<objective>Eliminate the enemy teams before 10 minutes is up</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="07fa10c6-f564-4630-861e-fe134ae27527"/> <!-- Yoyo_ -->
</authors>
<teams>
    <team id="red-team" color="dark red" max="8" max-overfill="12">Red</team>
    <team id="blue-team" color="blue" max="8" max-overfill="12">Blue</team>
    <team id="yellow-team" color="yellow" max="8" max-overfill="12">Yellow</team>
    <team id="green-team" color="green" max="8" max-overfill="12">Green</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet locked="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" team-color="true" material="leather leggings"/>
        <boots locked="true" team-color="true" material="leather boots"/>
        <effect duration="3" amplifier="10">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="green-team" kit="spawn-kit">
        <region angle="-23,6,7">
            <cuboid min="-56,8,-31" max="-54,8,-29"/>
        </region>
    </spawn>
    <spawn team="yellow-team" kit="spawn-kit">
        <region angle="-23,6,7">
            <cuboid min="-56,8,46" max="-54,8,44"/>
        </region>
    </spawn>
    <spawn team="red-team" kit="spawn-kit">
        <region angle="-23,6,7">
            <cuboid min="9,8,-31" max="7,8,-29"/>
        </region>
    </spawn>
    <spawn team="blue-team" kit="spawn-kit">
        <region angle="-23,6,7">
            <cuboid min="9,8,46" max="7,8,44"/>
        </region>
    </spawn>
    <default>
        <region angle="-23,6,7">
            <cuboid min="9,8,46" max="7,8,44"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>10m</time>
<blitz>
    <lives>2</lives>
    <broadcastLives>true</broadcastLives>
</blitz>
<rage/>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<timelock>on</timelock>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn auto="true"/>
</map>
