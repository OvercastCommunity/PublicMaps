<?xml version="1.0"?>
<map proto="1.3.6">
<name>Secluded Wharf</name>
<version>1.0.2</version>
<objective>Eliminate the other team before 5 minutes are up</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="07fa10c6-f564-4630-861e-fe134ae27527"/> <!-- Yoyo_ -->
</authors>
<contributors>
    <contributor uuid="fc98a4c4-1c1a-4d1d-acd3-a759ed436f26" contribution="Helping with interiors"/> <!-- hygey -->
</contributors>
<blitz>
    <time>5m</time>
    <lives>1</lives>
    <broadcastLives>false</broadcastLives>
</blitz>
<!-- include src="blitz-global.xml"/ -->
<respawn auto="true"/>
<rage/>
<kits>
    <kit name="spawn">
        <item slot="0" enchantment= "damage all:1000">fishing_rod</item>
        <item slot="1" enchantment="arrow_damage:10">bow</item>
        <item slot="2" amount="1">arrow</item>
        <item slot="3" amount="16">boat</item>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="cd0000">leather helmet</helmet>
        <chestplate color="cd0000">leather chestplate</chestplate>
        <leggings color="cd0000">leather leggings</leggings>
        <boots color="cd0000">leather boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="0066cc">leather helmet</helmet>
        <chestplate color="0066cc">leather chestplate</chestplate>
        <leggings color="0066cc">leather leggings</leggings>
        <boots color="0066cc">leather boots</boots>
    </kit>
</kits>
<toolrepair>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>boat</item>
    <item>arrow</item>
    <item>fishing rod</item>
</itemremove>
<killreward>
    <item amount="1">arrow</item>
</killreward>
<spawns>
    <spawn team="blue" kit="blue">
        <regions yaw="135">
            <cuboid min="-692,7,-1105" max="-689,7,-1102"/>
        </regions>
    </spawn>
    <spawn team="red" kit="red">
        <regions yaw="-45">
            <cuboid min="-724,7,-1189" max="-721,7,-1186"/>
        </regions>
    </spawn>
    <default yaw="-90">
        <regions yaw="-90">
            <point>-741.5,37,-1145.5</point>
        </regions>
    </default>
</spawns>
<regions>
    <apply block="deny-all">
        <rectangle name="map" min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<teams>
    <team color="red" max="16" max-overfill="20">Red</team>
    <team color="blue" max="16" max-overfill="20">Blue</team>
</teams>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
