<map proto="1.5.0">
<name>Secluded Wharf</name>
<version>1.0.4</version>
<objective>Eliminate the other team before 5 minutes are up</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="07fa10c6-f564-4630-861e-fe134ae27527"/> <!-- Yoyo_ -->
</authors>
<contributors>
    <contributor uuid="fc98a4c4-1c1a-4d1d-acd3-a759ed436f26" contribution="Helping with interiors"/> <!-- hygey -->
</contributors>
<teams>
    <team id="red" color="red" max="16" max-overfill="20">Red</team>
    <team id="blue" color="blue" max="16" max-overfill="20">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="fishing rod"/>
        <item slot="1" enchantment="arrow_damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <item slot="3" amount="16" material="boat"/>
        <helmet team-color="true" material="leather helmet"/>
        <chestplate team-color="true" material="leather chestplate"/>
        <leggings team-color="true" material="leather leggings"/>
        <boots team-color="true" material="leather boots"/>
    </kit>
</kits>
<spawns>
    <spawn team="blue" kit="spawn-kit">
        <region yaw="135">
            <cuboid min="-692,7,-1105" max="-689,7,-1102"/>
        </region>
    </spawn>
    <spawn team="red" kit="spawn-kit">
        <region yaw="-45">
            <cuboid min="-724,7,-1189" max="-721,7,-1186"/>
        </region>
    </spawn>
    <default>
        <region yaw="-90">
            <point>-741.5,37,-1145.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
    <broadcastLives>false</broadcastLives>
</blitz>
<rage/>
<toolrepair>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>boat</item>
    <item>arrow</item>
    <item>fishing rod</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn auto="true"/>
</map>
