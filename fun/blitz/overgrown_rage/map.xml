<map proto="1.5.0">
<name>Overgrown Rage</name>
<version>2.0.6</version>
<objective>Eliminate the opposing team in under 8 minutes.</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="c65e66b3-e932-4b33-b907-fe4916806076"/> <!-- Draco_Rogue -->
</authors>
<contributors>
    <contributor uuid="afb49ef2-888e-4280-857a-04cfac39e25d" contribution="Map Feedback"/> <!-- Pr0tatoe -->
</contributors>
<teams>
    <team id="blue" color="blue" max="30">Blue</team>
    <team id="red" color="dark red" max="30">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" name="`e`l1-Hit kill!" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" name="`e`l1-Shot Kill!" material="bow"/>
        <item slot="2" material="arrow"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <effect duration="5">heal</effect>
        <effect duration="5" amplifier="4">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="blue" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="-42,134,58" max="-37,134,63"/>
        </region>
    </spawn>
    <spawn team="red" kit="spawn-kit">
        <region>
            <cuboid min="-38,134,-58" max="-41,134,-61"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <cuboid min="95,166,-2" max="92,166,3"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>8m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<itemremove>
    <item>leather chestplate</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
