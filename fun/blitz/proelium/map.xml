<map proto="1.5.0">
<name>Proelium</name>
<version>1.0.9</version>
<objective>Kill the enemy team before time is up.</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="060baa18-2852-40d8-afcb-e61607c04be3"/> <!-- pepsidawg00 -->
</authors>
<contributors>
    <contributor uuid="22ba442b-cf26-4d09-b290-8d4e50a551b2" contribution="Name and XML beautification"/> <!-- Dinner1111 -->
    <contributor uuid="dad8b95c-cf6a-44df-982e-8c8dd70201e0" contribution="XML Refinification"/> <!-- ElectroidFilms -->
    <contributor uuid="e37f18f1-117b-47db-8806-6d3a0854b518" contribution="XML Helpification"/> <!-- chickolympics -->
    <contributor uuid="d341db34-f1ab-406e-b290-da6a6703565e" contribution="Minor map help"/> <!-- MlgCheetoPuff -->
    <contributor uuid="c1ea235d-4224-46e4-ba4a-638838f6d039" contribution="House design inspiration"/> <!-- Daffy_Duck01 -->
</contributors>
<teams>
    <team id="blue" color="blue" max="20">Blue</team>
    <team id="red" color="dark red" max="20">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="3" amount="8" material="bread"/>
        <item slot="2" material="arrow"/>
        <item slot="8" material="ender pearl"/>
        <helmet locked="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" team-color="true" material="leather leggings"/>
        <boots locked="true" team-color="true" material="leather boots"/>
        <effect>speed</effect>
        <effect duration="5" amplifier="25">heal</effect>
        <effect duration="5" amplifier="25">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
    <kit id="obs">
        <effect>night vision</effect>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region>
            <cuboid min="5,21,-232" max="2,21,-235"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="1,21,-58" max="6,21,-52"/>
        </region>
    </spawn>
    <default kit="obs" yaw="90">
        <region>
            <cylinder base="-121,44,-144" radius="2" height="0"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>2</lives>
</blitz>
<rage/>
<itemremove>
    <item>bread</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
</toolrepair>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
