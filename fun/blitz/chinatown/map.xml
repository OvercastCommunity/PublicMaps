<map proto="1.5.0">
<name>Chinatown</name>
<version>1.1.1</version>
<created>2015-10-12</created>
<objective>Eliminate the enemy team within 5 minutes!</objective>
<authors>
    <author uuid="a37e4da3-1c6c-4dc9-b085-63783406a963"/> <!-- candcshow -->
    <author uuid="d943b497-0b4b-4eb6-a5e1-3ba0b3fd57a0"/> <!-- inano2000 -->
</authors>
<teams>
    <team id="yellow" color="yellow" max="10" max-overfill="12">Yellow</team>
    <team id="blue" color="blue" max="10" max-overfill="12">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" unbreakable="true" enchantment="damage all:10" material="iron sword"/>
        <item slot="1" unbreakable="true" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet locked="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" material="chainmail leggings"/>
        <boots locked="true" team-color="true" material="leather boots"/>
    </kit>
</kits>
<spawns>
    <spawn team="yellow" kit="spawn-kit">
        <region yaw="180">
            <block>9,14,-41</block>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit">
        <region>
            <block>9,14,42</block>
        </region>
    </spawn>
    <default>
        <region yaw="-90">
            <block>64,50,3</block>
        </region>
    </default>
</spawns>
<regions>
    <apply block-break="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
    <broadcastLives>true</broadcastLives>
</blitz>
<rage/>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<itemremove>
    <item>iron sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
</map>
