<map proto="1.5.0">
<name>Ieats Hall</name>
<version>1.0.3</version>
<objective>Kill the other team before 5 minutes are up</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="36dc036a-dfbc-4627-b66a-501223f08075" contribution="Nearly everything"/> <!-- ieatboulders2 -->
</authors>
<contributors>
    <contributor uuid="2a289d2a-d970-49c5-9a6c-01fc0264e317" contribution="Skylights"/> <!-- Stealth5061 -->
    <contributor uuid="76cf5483-a7ea-4b5d-8830-385f768c4807" contribution="Other stuff"/> <!-- xannallax33 -->
</contributors>
<teams>
    <team id="red" color="dark red" max="16">Red</team>
    <team id="blue" color="blue" max="16">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" lore="`71-Hit Kill!" material="diamond sword"/>
        <item slot="1" enchantment="arrow damage:10" lore="`71-Shot Kill!" material="bow"/>
        <item slot="2" material="arrow"/>
        <item slot="3" amount="4" material="cooked beef"/>
        <helmet unbreakable="true" locked="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" locked="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" locked="true" team-color="true" material="leather leggings"/>
        <boots unbreakable="true" locked="true" team-color="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit" yaw="90">
        <region>
            <cuboid min="61,26,0" max="62,26,1"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="-89">
        <region>
            <cuboid min="-16,26,-121" max="-15,26,-120"/>
        </region>
    </spawn>
    <default yaw="90">
        <region>
            <cuboid min="-17,57,-62" max="-15,57,-60"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<toolrepair>
    <tool>diamond sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>cooked beef</item>
</itemremove>
<timelock>on</timelock>
</map>
