<map proto="1.5.0">
<name>Rooftops</name>
<version>1.0.3</version>
<objective>Eliminate the opposing team within 8 minutes.</objective>
<created>2013-07-12</created>
<include id="void-death"/>
<constants>
    <constant id="void-plane">87</constant>
    <constant id="affect-obs">never</constant>
</constants>
<authors>
    <author uuid="62b6d081-8df8-4c09-aa15-71141ca7df76"/> <!-- Arzanex -->
</authors>
<contributors>
    <contributor uuid="9d4779a8-d35f-45d2-b51c-95de7a77e128" contribution="XML"/> <!-- zzuf -->
</contributors>
<teams>
   <team id="purple-team" color="dark purple" max="20">Purple</team>
   <team id="cyan-team" color="dark aqua" max="20">Cyan</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" material="bow"/>
        <item slot="2" amount="8" material="arrow"/>
        <item slot="8" amount="4" material="bread"/>
        <helmet team-color="true" unbreakable="true" material="leather helmet"/>
        <chestplate team-color="true" unbreakable="true" material="leather chestplate"/>
        <leggings team-color="true" unbreakable="true" material="leather leggings"/>
        <boots team-color="true" unbreakable="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
        <effect duration="3s" amplifier="255">damage resistance</effect>
    </kit>
</kits>
<spawns>
    <spawn team="cyan-team" kit="spawn-kit" yaw="-45">
        <region>
            <cylinder base="-42,91,-64" height="0" radius="2"/>
        </region>
    </spawn>
    <spawn team="purple-team" kit="spawn-kit" yaw="-135">
        <region>
            <cylinder base="-42,91,21" height="0" radius="2"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <point>-27.5,129,-21.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>8m</time>
<blitz>
    <lives>2</lives>
    <broadcastLives>true</broadcastLives>
</blitz>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<killreward>
    <item amount="8" material="arrow"/>
</killreward>
<hunger>
    <depletion>on</depletion>
</hunger>
<respawn auto="true"/>
</map>
