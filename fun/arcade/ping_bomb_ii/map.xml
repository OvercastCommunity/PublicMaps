<map proto="1.5.0">
<name>Ping Bomb II</name>
<version>1.2.8</version>
<created>2017-01-14</created>
<objective>Blow up the enemy side before they blow up yours!</objective>
<gamemode>arcade</gamemode>
<gamemode>blitz</gamemode>
<authors>
    <author uuid="f5d5c301-9ad3-4cb4-9cfd-a6b78e67734a" contribution="Design"/> <!-- Flashee -->
    <author uuid="17feaec5-9616-49d8-a507-83026050dd8f" contribution="Design"/> <!-- ShinyDialga -->
    <author uuid="453003c1-d3a2-4b0a-b491-1205e503de90" contribution="Idea"/> <!-- majjus_ -->
</authors>
<teams>
    <team id="red" color="dark red" max="25">Red</team>
    <team id="blue" color="blue" max="25">Blue</team>
</teams>
<kits>
    <kit id="spawn" parents="obs">
        <if max-server-version="1.8">
            <item slot="0" amount="12" material="sandstone"/>
        </if>
        <if min-server-version="1.9">
            <item slot="0" amount="12" material="end stone bricks"/>
        </if>
        <effect amplifier="-3">health boost</effect>
    </kit>
    <kit id="obs">
        <effect>night vision</effect>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn">
        <region>
            <cuboid min="2,26,-57" max="-2,26,-61"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn">
        <region yaw="180">
            <cuboid min="-2,26,45" max="2,26,49"/>
        </region>
    </spawn>
    <default kit="obs">
        <region yaw="90">
            <cuboid min="38,39,5" max="40,39,-17"/>
        </region>
    </default>
</spawns>
<filters>
    <any id="not-TNT">
        <material>stained clay</material>
        <material>step</material>
        <material>double step</material>
        <material>quartz stairs</material>
        <material>quartz block</material>
        <material>sandstone</material>
        <material>coal block</material>
        <material>barrier</material>
        <material>skull</material>
        <material>mob spawner</material>
        <material>fire</material>
    </any>
</filters>
<regions>
    <apply block-break="not-TNT" message="You may not break TNT!"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
</blitz>
<falling-blocks>
    <rule>
        <filter>
            <material>stained clay</material>
        </filter>
        <sticky>
            <any>
                <material>tnt</material>
                <material>stained clay</material>
                <material>quartz stairs</material>
            </any>
        </sticky>
    </rule>
    <rule>
        <filter>
            <material>quartz block</material>
        </filter>
        <sticky>
            <any>
                <material>stained clay</material>
                <material>quartz stairs</material>
                <material>quartz block</material>
            </any>
        </sticky>
    </rule>
    <rule>
        <filter>
            <material>quartz stairs</material>
        </filter>
        <sticky>
            <any>
                <material>stained clay</material>
                <material>quartz stairs</material>
                <material>quartz block</material>
            </any>
        </sticky>
    </rule>
    <rule>
        <filter>
            <material>double step</material>
        </filter>
        <sticky>
            <any>
                <material>stained clay</material>
            </any>
        </sticky>
    </rule>
    <rule>
        <filter>
            <material>step</material>
        </filter>
        <sticky>
            <any>
                <material>stained clay</material>
                <material>barrier</material>
                <material>step</material>
            </any>
        </sticky>
    </rule>
    <rule>
        <filter>
            <material>barrier</material>
        </filter>
        <sticky>
            <any>
                <material>stained clay</material>
            </any>
        </sticky>
    </rule>
    <rule>
        <filter>
            <material>coal block</material>
        </filter>
        <sticky>
            <any>
                <material>tnt</material>
            </any>
        </sticky>
    </rule>
    <if max-server-version="1.8">
        <rule>
            <filter>
                <material>sandstone</material>
            </filter>
            <sticky>
                <any>
                    <material>stained clay</material>
                    <material>step</material>
                    <material>double step</material>
                    <material>quartz stairs</material>
                    <material>quartz block</material>
                    <material>sandstone</material>
                </any>
            </sticky>
        </rule>
    </if>
    <if min-server-version="1.9">
        <rule>
            <filter>
                <material>end stone bricks</material>
            </filter>
            <sticky>
                <any>
                    <material>stained clay</material>
                    <material>step</material>
                    <material>double step</material>
                    <material>quartz stairs</material>
                    <material>quartz block</material>
                    <material>end stone bricks</material>
                </any>
            </sticky>
        </rule>
    </if>
</falling-blocks>
<kill-rewards>
    <kill-reward>
        <item material="diamond"/>
    </kill-reward>
</kill-rewards>
<gamerules>
    <naturalRegeneration>false</naturalRegeneration>
</gamerules>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
