        <boots color="F27FA5" material="leather boots"/>
        <effect>speed</effect>
    </kit>
    <kit id="defender-kit">
        <item slot="1" unbreakable="true" enchantment="arrow knockback;arrow damage:3;arrow infinite" material="bow"/>
        <item slot="28" material="arrow"/>
